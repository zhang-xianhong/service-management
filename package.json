{
  "name": "service-architecture-management",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "husky": "^4.3.8",
    "lerna": "^3.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run --concurrency 1 --stream precommit --since HEAD",
